{
    "extends": "astro/tsconfigs/base",
    "compilerOptions": {
        "noImplicitAny": false,
        "checkJs": false,
        "skipLibCheck": true,
        "jsx": "preserve",
        "baseUrl": ".",
        "paths": {
            "@atoms/*": ["src/atoms/*"],
            "@blocks/*": ["src/blocks/*"],
            "@layouts/*": ["src/layouts/*"],
            "@composites/*": ["src/composites/*"],
            "@templates/*": ["src/templates/*"],
            "@content/*": ["src/content/*"],
            "@scripts/*": ["src/scripts/*"],
            "@styles/*": ["src/styles/*"],
        }
    }
}