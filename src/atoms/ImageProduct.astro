---
import { Image } from 'astro:assets'

const {
    filename,
    class: classes,
    ...attrs
} = Astro.props
---
<Image
    src={ filename }
    alt={ '' }
    width="576"
    height="576"
    quality="100"
    draggable="false"
    class:list={[
        'object-contain aspect-square max-w-52',
        classes
    ]}
    { ...attrs }
/>