---
import { Image } from 'astro:assets'

const {
    filename,
    class: classes,
    ...attrs
} = Astro.props
---
<Image
    src={ filename }
    alt={ '' }
    width="1440"
    height="1440"
    quality="100"
    draggable="false"
    class:list={[ 'object-cover aspect-square', classes ]}
    { ...attrs }
/>