---
const { links } = Astro.props
---
{
    links.map((link: any) => {
        return (
            <a href={ `/${link.link.story.full_slug == 'home' ? '' : link.link.story.full_slug}` }>
                <div>
                    { link.title }
                </div>
            </a>
        )
    })
}